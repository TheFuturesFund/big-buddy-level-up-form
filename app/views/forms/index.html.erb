<p id="notice"><%= notice %></p>

<h1>Listing Forms</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Cellphone number</th>
      <th>Date of birth</th>
      <th>Social security number</th>
      <th>Gender</th>
      <th>Email addresses</th>
      <th>School</th>
      <th>Grade</th>
      <th>Race</th>
      <th>Indiviual graduation plan</th>
      <th>Special education services</th>
      <th>Free lunch</th>
      <th>Parent in prison</th>
      <th>Esl services</th>
      <th>Tshirt size</th>
      <th>Doctor name</th>
      <th>Doctor workphone</th>
      <th>Medical restrictions</th>
      <th>Medical restrictions explained</th>
      <th>Medications</th>
      <th>Medications explained</th>
      <th>Allergies</th>
      <th>Allergies explained</th>
      <th>Guardian1 contact name</th>
      <th>Guardian1 contact employment</th>
      <th>Guardian1 contact workphone</th>
      <th>Guardian1 contact homephone</th>
      <th>Guardian1 contact cellphone</th>
      <th>Guardian1 contact email</th>
      <th>Emergency contact name</th>
      <th>Emergency contact relationship</th>
      <th>Emergency contact workphone</th>
      <th>Emergency contact cellphone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forms.each do |form| %>
      <tr>
        <td><%= form.first_name %></td>
        <td><%= form.middle_name %></td>
        <td><%= form.last_name %></td>
        <td><%= form.address %></td>
        <td><%= form.city %></td>
        <td><%= form.state %></td>
        <td><%= form.zipcode %></td>
        <td><%= form.cellphone_number %></td>
        <td><%= form.date_of_birth %></td>
        <td><%= form.social_security_number %></td>
        <td><%= form.gender %></td>
        <td><%= form.email_addresses %></td>
        <td><%= form.school %></td>
        <td><%= form.grade %></td>
        <td><%= form.race %></td>
        <td><%= form.indiviual_graduation_plan %></td>
        <td><%= form.special_education_services %></td>
        <td><%= form.free_lunch %></td>
        <td><%= form.parent_in_prison %></td>
        <td><%= form.ESL_services %></td>
        <td><%= form.tshirt_size %></td>
        <td><%= form.doctor_name %></td>
        <td><%= form.doctor_workphone %></td>
        <td><%= form.medical_restrictions %></td>
        <td><%= form.medical_restrictions_explained %></td>
        <td><%= form.medications %></td>
        <td><%= form.medications_explained %></td>
        <td><%= form.allergies %></td>
        <td><%= form.allergies_explained %></td>
        <td><%= form.guardian1_contact_name %></td>
        <td><%= form.guardian1_contact_employment %></td>
        <td><%= form.guardian1_contact_workphone %></td>
        <td><%= form.guardian1_contact_homephone %></td>
        <td><%= form.guardian1_contact_cellphone %></td>
        <td><%= form.guardian1_contact_email %></td>
        <td><%= form.emergency_contact_name %></td>
        <td><%= form.emergency_contact_relationship %></td>
        <td><%= form.emergency_contact_workphone %></td>
        <td><%= form.emergency_contact_cellphone %></td>
        <td><%= link_to 'Show', form %></td>
        <td><%= link_to 'Edit', edit_form_path(form) %></td>
        <td><%= link_to 'Destroy', form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

